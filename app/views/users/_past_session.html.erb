<div class="title col-xs-8 col-sm-9 col-lg-9 mx-auto mt-5" style='display: block'>
  <h3>Past Sessions</h3>
</div>
<div class="container">
  <% bookings.each do |booking| %>
    <div class="session-card row mx-auto my-3 p-3 d-flex justify-content-center">
      <div class="counselor_pic col-xs-2 col-sm-3 col-lg-3 text-center">
        <%= cl_image_tag(booking.counselor.photo.key, height: 80, width: 80, radius: 'max', background: '#f4f4f4') %>
        <p>with <strong><%= booking.counselor.last_name %> <%= booking.counselor.first_name %></strong></p>
      </div>
      <div class="counselor_info col-xs-6 col-sm-6 col-lg-6 text-left">
        <% date = booking.start_time %>
        <%= render "shared/session_card_info", booking: booking %>
      </div>
    </div>
  <% end %>
</div>
